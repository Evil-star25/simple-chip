#! /opt/homebrew/Cellar/icarus-verilog/12.0/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/system.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_sys.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_textio.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/v2005_math.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x135e061a0 .scope module, "pwm_tb" "pwm_tb" 2 4;
 .timescale -9 -12;
v0x135e169c0_0 .var "clk", 0 0;
v0x135e16a60_0 .var "duty_cycle", 7 0;
v0x135e16b10_0 .var/i "file", 31 0;
v0x135e16bc0_0 .net "pwm_out", 0 0, v0x135e16800_0;  1 drivers
v0x135e16c70_0 .var "reset", 0 0;
E_0x135e06310 .event posedge, v0x135e065e0_0;
S_0x135e06350 .scope module, "uut" "pwm" 2 13, 3 1 0, S_0x135e061a0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 8 "duty_cycle";
    .port_info 2 /INPUT 1 "reset";
    .port_info 3 /OUTPUT 1 "pwm_out";
v0x135e065e0_0 .net "clk", 0 0, v0x135e169c0_0;  1 drivers
v0x135e16690_0 .var "counter", 7 0;
v0x135e16740_0 .net "duty_cycle", 7 0, v0x135e16a60_0;  1 drivers
v0x135e16800_0 .var "pwm_out", 0 0;
v0x135e168a0_0 .net "reset", 0 0, v0x135e16c70_0;  1 drivers
E_0x135e06580 .event posedge, v0x135e168a0_0, v0x135e065e0_0;
    .scope S_0x135e06350;
T_0 ;
    %wait E_0x135e06580;
    %load/vec4 v0x135e168a0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x135e16690_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x135e16800_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x135e16690_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x135e16690_0, 0;
    %load/vec4 v0x135e16690_0;
    %load/vec4 v0x135e16740_0;
    %cmp/u;
    %flag_mov 8, 5;
    %jmp/0 T_0.2, 8;
    %pushi/vec4 1, 0, 1;
    %jmp/1 T_0.3, 8;
T_0.2 ; End of true expr.
    %pushi/vec4 0, 0, 1;
    %jmp/0 T_0.3, 8;
 ; End of false expr.
    %blend;
T_0.3;
    %assign/vec4 v0x135e16800_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x135e061a0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x135e169c0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x135e16c70_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x135e061a0;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x135e169c0_0;
    %inv;
    %store/vec4 v0x135e169c0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x135e061a0;
T_3 ;
    %vpi_func 2 25 "$fopen" 32, "pwm_output.txt", "w" {0 0 0};
    %store/vec4 v0x135e16b10_0, 0, 32;
    %load/vec4 v0x135e16b10_0;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_3.0, 4;
    %vpi_call 2 27 "$display", "Failed to open file!" {0 0 0};
    %vpi_call 2 28 "$finish" {0 0 0};
T_3.0 ;
    %vpi_call 2 32 "$fwrite", v0x135e16b10_0, "Time\011Clk\011PWM_out\012" {0 0 0};
    %pushi/vec4 64, 0, 8;
    %store/vec4 v0x135e16a60_0, 0, 8;
    %vpi_call 2 37 "$display", "Time\011Clk\011Counter\011PWM_out" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x135e16c70_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x135e16c70_0, 0, 1;
    %pushi/vec4 600, 0, 32;
T_3.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.3, 5;
    %jmp/1 T_3.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x135e06310;
    %vpi_call 2 45 "$fwrite", v0x135e16b10_0, "%0t\011%b\011%b\012", $time, v0x135e169c0_0, v0x135e16bc0_0 {0 0 0};
    %jmp T_3.2;
T_3.3 ;
    %pop/vec4 1;
    %vpi_call 2 48 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x135e061a0;
T_4 ;
    %wait E_0x135e06310;
    %vpi_call 2 53 "$display", "%0dns\011%b\011%d\011%b", $time, v0x135e169c0_0, v0x135e16690_0, v0x135e16bc0_0 {0 0 0};
    %jmp T_4;
    .thread T_4;
# The file index is used to find the file name in the following table.
